generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Media {
  id            String   @id @default(uuid())
  title         String
  size          String
  url           String
  hasTranscript Boolean  @default(false)
  hasSubtitle   Boolean  @default(false)
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
